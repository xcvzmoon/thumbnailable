{
  "name": "thumbnailable",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "format": "oxfmt && oxfmt --check",
    "lint": "oxlint --type-aware",
    "dev": "bun --watch ./src/index.ts",
    "build": "bunx --bun tsdown",
    "start": "bun run ./dist/index.mjs",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "changelog:bump": "changelogen --bump",
    "changelog:release": "changelogen --release"
  },
  "keywords": [
    "thumbnail",
    "document",
    "undms"
  ],
  "author": {
    "name": "Mon Albert Gamil",
    "email": "mrgamilmonalbert@gmail.com",
    "url": "https://github.com/xcvzmoon/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xcvzmoon/thumbnailable"
  },
  "sideEffects": false,
  "license": "MIT",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.3.0",
    "@types/pdfjs-dist": "^2.10.378",
    "changelogen": "^0.6.2",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.48.0",
    "oxlint-tsgolint": "^0.11.5",
    "pdfjs-dist": "^5.4.624",
    "tsdown": "^0.20.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.9",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.93",
    "exiftool-vendored": "^35.9.0",
    "sharp": "^0.34.5",
    "unpdf": "^1.4.0"
  }
}
